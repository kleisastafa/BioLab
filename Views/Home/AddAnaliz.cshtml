@model Analiza
<partial name="NavPartial"/>
<div class="mt-5 container">
        <h1 class="text-center mb-5">Krijo nje analize</h1>
    
   

<div class="row">
  <form  asp-controller="Home" asp-action="CreateAnaliz"  method="post">
    <div class="col-sm-6">
      

              <div class="form-group">
                  <span asp-validation-for="Emri"></span>
                  <label asp-for="Emri"></label>
                  <input asp-for="Emri" class="form-control" >
                
                </div>
                <div class="form-group">
                  <span asp-validation-for="Njesia"></span>
                  <label asp-for="Njesia"></label>
                  <input  asp-for="Njesia" class="form-control" >
                
                </div>
                  <div class="form-group">
                  <span asp-validation-for="Norma"></span>
                  <label asp-for="Norma"></label>
                  <input  asp-for="Norma" class="form-control" >
                
                </div>
                <div class="form-group">
                  <span asp-validation-for="Cmimi"></span>
                  <label asp-for="Cmimi"></label>
                  <input  asp-for="Cmimi" class="form-control" >
                
                </div>
    </div>
        
    <div class="col-sm-6">
      <button type="submit" class="btn btn-primary mt-3">Krijo Analize</button>
      <a asp-controller="Home" asp-action="Index" class="btn btn-primary mt-3 mr-3" > Kthehu pas</a>
      </div>
</form>
</div>





    