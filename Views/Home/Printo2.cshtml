<div class="mt-5 container">
    <h3>Emri Pacientit : @ViewBag.thisflet.MyPacient.Emripacientit</h3>
    <h3>Seksi : @ViewBag.thisflet.MyPacient.Gjinia</h3>
    <h3>Tipi pacientit : @ViewBag.thisflet.MyPacient.Tipi</h3>
    <h3>Mosha: @ViewBag.thisflet.MyPacient.Mosha</h3>
    <h3>Data: @ViewBag.thisflet.CreatedAt.ToString(string.Format("dd/MM/yyyy"))</h3>
    <div class="row">
        <div class="col sm-6">

            <table class="table table-striped table-bordered  ">

                <thead>
                    <tr>
                        <th scope="col ">Emri</th>
                        <th scope="col">Njesia</th>
                        <th scope="col">Vlera</th>
                        <th scope="col">Norma</th>



                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in ViewBag.thisflet.mtms)

                    {
                        <tr>
                            <td scope="row"> @item.Myanaliz.Emri</td>
                            <td scope="row"> @item.Myanaliz.Njesia</td>
                            <td scope="row"> @item.Myanaliz.Rezultati</td>
                            <td scope="row">
                                @item.Myanaliz.Norma
                            </td>



                        </tr>

                    }

                </tbody>


            </table>
            <button onclick="window.print()">Print this page</button>
        </div>
    </div>
</div>